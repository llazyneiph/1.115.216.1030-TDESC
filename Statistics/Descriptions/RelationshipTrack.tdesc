        <TunableVariant type="None" name="bit_data_tuning" class="TunableVariant" default="none" display="Bit Data Tuning" muid="6ED589A515F51DCA" description="Bit tuning for all the bits that compose this relationship &#xA;track.&#xA;The structure tuned here, either 2d or simple track should include &#xA;bits for all the possible range of the track." Deprecated="False" filter="0" group="General">
            <TunableTuple name="_2dMatrix" class="TunableRelationshipTrack2dLink" display="2Dmatrix" muid="EE9A0FD826316038" Deprecated="False" filter="0" group="General">
                <TunableList name="y_axis_content" class="TunableList" display="Y Axis Content" muid="563C1E47015A4E12" description="A list of bit sets and thresholds.  This represents the Y axis of the matrix." Deprecated="False" filter="0" group="General">
                    <TunableTuple class="TunableTuple" muid="AF0347B364ABE2E3" description="A threshold for this node in the matrix along with a bit set." Deprecated="False" filter="0" group="General">
                        <Tunable type="float" name="add_value" class="Tunable" default="100" display="Add Value" muid="A44FBE195A91DB36" description="Track score value for the bit to be added." tuning_state="NeedsTuning" Deprecated="False" filter="0" group="General" />
                        <TunableList name="bit_set" class="TunableRelationshipBitSet" display="Bit Set" muid="7E8981CFF8C14D0" description="The bit set representing the X axis in the matrix for this Y position." Deprecated="False" filter="0" group="General">
                            <TunableTuple class="TunableTuple" muid="851095D09C985DC9" description="Data for this bit in the track" Deprecated="False" filter="0" group="General">
                                <Tunable type="float" name="add_value" class="Tunable" default="100" display="Add Value" muid="64A6FCE2D87E96FE" description="Track score value for the bit to be added.&#xA;Since by default all relationships will converge at 0 we &#xA;must tune depending on the side of the zero we are in.&#xA;For values greater than 0, this must be greater than remove &#xA;value.&#xA;For values less than 0 this must be less than remove value. &#xA;&#xA;Example: For the friendship track:&#xA;GOOD_FRIENDS (value&gt;0) has an add value of 60&#xA;As soon as the track value goes &gt;= 60 the bit good friends&#xA;will be added and the previous active track bit will be removed.&#xA;&#xA;DISLIKED (value&lt;0) has an add_value of -20&#xA;As soon as the track value goes &lt;= -20 the bit disliked will &#xA;be added and the previous active track bid will be removed.&#xA;&#xA;TUNING MIDDLE VALUES (Ranges approach 0)&#xA;When tuning a value that goes past 0 (a bit from 10 to -10) it&#xA;is recommended we tune a positive Bit (10 to 0) and a negative &#xA;bit (-10 to 0).  This way, we can guarantee the rules will &#xA;consider correct positive and negative directions." tuning_state="NeedsTuning" Deprecated="False" filter="0" group="General" />
                                <Tunable type="relbit" name="bit" class="TunableReference" display="Bit" muid="7DCDDBBC3AE481E7" description="Reference to bit in set" allow_none="False" Deprecated="False" filter="0" group="General" />
                                <Tunable type="float" name="remove_value" class="Tunable" default="-100" display="Remove Value" muid="A6035555B1BF75C7" description="Track score value for the bit to be removed.&#xA;Since by default all relationships will converge at 0 we &#xA;must tune depending on the side of the zero we are in.&#xA;For values greater than 0, this must be less than add value.&#xA;For values less than 0, this must be greater than add value. &#xA;&#xA;For example, on the friendship track:&#xA;GOOD_FRIENDS (value&gt;0) has a remove value of 55.&#xA;As soon as the track value goes below 55 the bit good friends&#xA;will be removed, and the next lowest bit will be added.&#xA;&#xA;DISLIKED (value&lt;0) has a remove_value of -15.&#xA;As soon as the track value goes over -15 the bit disliked will &#xA;be removed and the next highest bit will be added.&#xA;&#xA;TUNING MIDDLE VALUES (Ranges approach 0)&#xA;When tuning a value that goes past 0 (a bit from 10 to -10) it&#xA;is recommended we tune a positive Bit (10 to 0) and a negative &#xA;bit (-10 to 0).  This way, we can guarantee the rules will &#xA;consider correct positive and negative directions." tuning_state="NeedsTuning" Deprecated="False" filter="0" group="General" />
                            </TunableTuple>
                        </TunableList>
                        <Tunable type="float" name="remove_value" class="Tunable" default="-100" display="Remove Value" muid="F69A8B08B4EE8A5E" description="Track score value for the bit to be removed." tuning_state="NeedsTuning" Deprecated="False" filter="0" group="General" />
                    </TunableTuple>
                </TunableList>
                <Tunable type="statistic" name="y_axis_track" class="TunableReference" display="Y Axis Track" muid="4369191E97F583C4" description="The bit track to key the Y axis off of." allow_none="False" Deprecated="False" filter="0" group="General" restrict="RelationshipTrack" />
            </TunableTuple>
            <TunableTuple name="bit_intervals" class="TunableTunableRelationshipIntervalBitData" display="Bit Intervals" muid="5DC56D4D7CAA98AC" description="Tuning to represent a track's bit tuning." Deprecated="False" filter="0" group="General">
                <TunableList name="entries" class="TunableList" display="Entries" muid="95569D852F7D8423" description="The collection of bit intervals to use for this track." Deprecated="False" filter="0" group="General">
                    <TunableTuple class="TunableTunableRelationshipTrackBitInterval" muid="1D6CE2B3362BA881" description="Tuning to represent an interval of a track's score where the given bit is active." Deprecated="False" filter="0" group="General">
                        <Tunable type="relbit" name="bit" class="TunableReference" display="Bit" muid="79F9A131B1770EA9" description="The bit that we want to use in this interval." allow_none="False" Deprecated="False" filter="0" group="General" />
                        <TunableTuple name="interval" class="TunableInterval" display="Interval" muid="95EA7F1C63BD69F9" description="The interval that this bit should be active for." Deprecated="False" filter="0" group="General">
                            <Tunable type="float" name="lower_bound" class="TunableRange" default="-100" display="Lower Bound" muid="7F80FC927ED438E7" min="None" max="None" description="The lower bound of the interval." tuning_state="NeedsTuning" Deprecated="False" filter="0" group="General" />
                            <Tunable type="float" name="upper_bound" class="TunableRange" default="100" display="Upper Bound" muid="F726637DA07CFE5B" min="None" max="None" description="The upper bound of the interval." tuning_state="NeedsTuning" Deprecated="False" filter="0" group="General" />
                        </TunableTuple>
                        <Tunable type="float" name="stickiness" class="Tunable" default="0" display="Stickiness" muid="BC84C6A89DE22088" description="How far past the interval boundaries should we continue&#xA;using this bit before switching to the next bit?&#xA;&#xA;When transitioning between intervals, it can be useful&#xA;to have the previous value 'stick' beyond the boundaries&#xA;of the interval. This can help prevent the bits from&#xA;switching too frequently if the track's value fluctuates&#xA;around an interval boundary.&#xA;&#xA;For example,&#xA;if we're on bit A, which is tuned for interval -10 to 10,&#xA;with a stickiness of 5, we won't switch away from A until&#xA;we're either below -15 or above 15.&#xA;Once we've left A, we won't re-enter A until we go above&#xA;-10 or below 10, assuming no other bits have stickiness." tuning_state="NeedsTuning" Deprecated="False" filter="0" group="General" />
                    </TunableTuple>
                </TunableList>
            </TunableTuple>
            <TunableTuple name="bit_set" class="TunableRelationshipBitData" display="Bit Set" muid="157F6222612C7AE3" Deprecated="False" filter="0" group="General">
                <TunableList name="bit_data" class="TunableRelationshipBitSet" display="Bit Data" muid="E1DDFBC1E19554CE" Deprecated="False" filter="0" group="General">
                    <TunableTuple class="TunableTuple" muid="E4E882676DD39D0E" description="Data for this bit in the track" Deprecated="False" filter="0" group="General">
                        <Tunable type="float" name="add_value" class="Tunable" default="100" display="Add Value" muid="9DE7A5EDC246994A" description="Track score value for the bit to be added.&#xA;Since by default all relationships will converge at 0 we &#xA;must tune depending on the side of the zero we are in.&#xA;For values greater than 0, this must be greater than remove &#xA;value.&#xA;For values less than 0 this must be less than remove value. &#xA;&#xA;Example: For the friendship track:&#xA;GOOD_FRIENDS (value&gt;0) has an add value of 60&#xA;As soon as the track value goes &gt;= 60 the bit good friends&#xA;will be added and the previous active track bit will be removed.&#xA;&#xA;DISLIKED (value&lt;0) has an add_value of -20&#xA;As soon as the track value goes &lt;= -20 the bit disliked will &#xA;be added and the previous active track bid will be removed.&#xA;&#xA;TUNING MIDDLE VALUES (Ranges approach 0)&#xA;When tuning a value that goes past 0 (a bit from 10 to -10) it&#xA;is recommended we tune a positive Bit (10 to 0) and a negative &#xA;bit (-10 to 0).  This way, we can guarantee the rules will &#xA;consider correct positive and negative directions." tuning_state="NeedsTuning" Deprecated="False" filter="0" group="General" />
                        <Tunable type="relbit" name="bit" class="TunableReference" display="Bit" muid="98AF3FA7A99AE7EB" description="Reference to bit in set" allow_none="False" Deprecated="False" filter="0" group="General" />
                        <Tunable type="float" name="remove_value" class="Tunable" default="-100" display="Remove Value" muid="C64EB805B269BB85" description="Track score value for the bit to be removed.&#xA;Since by default all relationships will converge at 0 we &#xA;must tune depending on the side of the zero we are in.&#xA;For values greater than 0, this must be less than add value.&#xA;For values less than 0, this must be greater than add value. &#xA;&#xA;For example, on the friendship track:&#xA;GOOD_FRIENDS (value&gt;0) has a remove value of 55.&#xA;As soon as the track value goes below 55 the bit good friends&#xA;will be removed, and the next lowest bit will be added.&#xA;&#xA;DISLIKED (value&lt;0) has a remove_value of -15.&#xA;As soon as the track value goes over -15 the bit disliked will &#xA;be removed and the next highest bit will be added.&#xA;&#xA;TUNING MIDDLE VALUES (Ranges approach 0)&#xA;When tuning a value that goes past 0 (a bit from 10 to -10) it&#xA;is recommended we tune a positive Bit (10 to 0) and a negative &#xA;bit (-10 to 0).  This way, we can guarantee the rules will &#xA;consider correct positive and negative directions." tuning_state="NeedsTuning" Deprecated="False" filter="0" group="General" />
                    </TunableTuple>
                </TunableList>
            </TunableTuple>
            <Tunable name="none" class="TunableExistance" display="None" muid="9946E546A64D8795" />
        </TunableVariant>

        <Tunable type="bool" name="causes_delayed_removal_on_convergence" class="Tunable" default="False" display="Causes Delayed Removal On Convergence" muid="C92DA938C877860B" description="If True, this track may cause the relationship to get culled&#xA;when it reaches convergence.  This is not guaranteed, based on&#xA;the culling rules.  Sim relationships will NOT be culled if any&#xA;of the folling conditions are met: &#xA;- Sim has any relationship bits that are tuned to prevent this. &#xA;- The sims are in the same household&#xA;&#xA;Note: This value is ignored by the Relationship Culling Story&#xA;Progression Action." Deprecated="False" filter="0" group="~Core~" />

