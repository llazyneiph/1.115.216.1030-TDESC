<?xml version="1.0" encoding="iso-8859-1"?>

<TuningRoot>
    <Instance instance_type="statistic" class="Commodity" module="statistics.commodity" muid="106FC0AE6B4E6BE3" description="Represents any commodity that has a value and changes over time.&#xA;This class updates lazily, which means it generally only processes change&#xA;when either a new delta is added, a delta is removed,&#xA;or someone tries to inspect the value of the commodity." instance_needs_reference="True" instance_subclasses_only="False" path="Statistics\Descriptions" use_guid_for_reference="True">
        <Tunable type="bool" name="_add_if_not_in_tracker" class="Tunable" default="True" display="Add If Not In Tracker" muid="90FAE81F1B5651A7" description="If True, when we try to modify the value of the commodity, we will add&#xA;the commodity to the tracker if the tracker doesn't already have&#xA;it.&#xA;&#xA;e.g If a sim uses the toilet and we update bladder when that sim&#xA;doesn't have the bladder commodity in his/her tracker, we will&#xA;add the bladder commodity to that sim. &#xA;&#xA;Set this to false for the case of NPC behavior commodities like&#xA;Being a Maid or Being a Burglar." Deprecated="False" filter="0" group="General" />

        <Tunable type="bool" name="_affects_plumbob_color" class="Tunable" default="True" display="Affects Plumbob Color" muid="25EE83D7BE918496" description="Uncheck this if the state of this commodity should not affect the plumbob color.&#xA;This is useful if you want a sim's plumbob to stay green even if this commodity&#xA;state is low.&#xA;&#xA;This tuning is only relevant if this commodity is visible, as non-visible&#xA;commodities don't affect the plumbob anyway." Deprecated="False" filter="0" group="General" />

