        <TunableVariant type="None" name="bit_data_tuning" class="TunableVariant" default="none" display="Bit Data Tuning" muid="E6FA69C52527EB74" description="Bit tuning for all the bits that compose this relationship &#xA;track.&#xA;The structure tuned here, either 2d or simple track should include &#xA;bits for all the possible range of the track." Deprecated="False" filter="0" group="General">
            <TunableTuple name="_2dMatrix" class="TunableRelationshipTrack2dLink" display="2Dmatrix" muid="81F8E50367B8592A" Deprecated="False" filter="0" group="General">
                <TunableList name="y_axis_content" class="TunableList" display="Y Axis Content" muid="A14B62219FABFE47" description="A list of bit sets and thresholds.  This represents the Y axis of the matrix." Deprecated="False" filter="0" group="General">
                    <TunableTuple class="TunableTuple" muid="BED0DF514C8F30F2" description="A threshold for this node in the matrix along with a bit set." Deprecated="False" filter="0" group="General">
                        <Tunable type="float" name="add_value" class="Tunable" default="100" display="Add Value" muid="325819FB6AA0C412" description="Track score value for the bit to be added." tuning_state="NeedsTuning" Deprecated="False" filter="0" group="General" />
                        <TunableList name="bit_set" class="TunableRelationshipBitSet" display="Bit Set" muid="E1F9762A3B0DCA9" description="The bit set representing the X axis in the matrix for this Y position." Deprecated="False" filter="0" group="General">
                            <TunableTuple class="TunableTuple" muid="4D79D83F6EBAAB71" description="Data for this bit in the track" Deprecated="False" filter="0" group="General">
                                <Tunable type="float" name="add_value" class="Tunable" default="100" display="Add Value" muid="9706BAF2482EFFE8" description="Track score value for the bit to be added.&#xA;Since by default all relationships will converge at 0 we &#xA;must tune depending on the side of the zero we are in.&#xA;For values greater than 0, this must be greater than remove &#xA;value.&#xA;For values less than 0 this must be less than remove value. &#xA;&#xA;Example: For the friendship track:&#xA;GOOD_FRIENDS (value&gt;0) has an add value of 60&#xA;As soon as the track value goes &gt;= 60 the bit good friends&#xA;will be added and the previous active track bit will be removed.&#xA;&#xA;DISLIKED (value&lt;0) has an add_value of -20&#xA;As soon as the track value goes &lt;= -20 the bit disliked will &#xA;be added and the previous active track bid will be removed.&#xA;&#xA;TUNING MIDDLE VALUES (Ranges approach 0)&#xA;When tuning a value that goes past 0 (a bit from 10 to -10) it&#xA;is recommended we tune a positive Bit (10 to 0) and a negative &#xA;bit (-10 to 0).  This way, we can guarantee the rules will &#xA;consider correct positive and negative directions." tuning_state="NeedsTuning" Deprecated="False" filter="0" group="General" />
                                <Tunable type="relbit" name="bit" class="TunableReference" display="Bit" muid="E589D858C7045D78" description="Reference to bit in set" allow_none="False" Deprecated="False" filter="0" group="General" />
                                <Tunable type="float" name="remove_value" class="Tunable" default="-100" display="Remove Value" muid="B743FDE9ECC57AC2" description="Track score value for the bit to be removed.&#xA;Since by default all relationships will converge at 0 we &#xA;must tune depending on the side of the zero we are in.&#xA;For values greater than 0, this must be less than add value.&#xA;For values less than 0, this must be greater than add value. &#xA;&#xA;For example, on the friendship track:&#xA;GOOD_FRIENDS (value&gt;0) has a remove value of 55.&#xA;As soon as the track value goes below 55 the bit good friends&#xA;will be removed, and the next lowest bit will be added.&#xA;&#xA;DISLIKED (value&lt;0) has a remove_value of -15.&#xA;As soon as the track value goes over -15 the bit disliked will &#xA;be removed and the next highest bit will be added.&#xA;&#xA;TUNING MIDDLE VALUES (Ranges approach 0)&#xA;When tuning a value that goes past 0 (a bit from 10 to -10) it&#xA;is recommended we tune a positive Bit (10 to 0) and a negative &#xA;bit (-10 to 0).  This way, we can guarantee the rules will &#xA;consider correct positive and negative directions." tuning_state="NeedsTuning" Deprecated="False" filter="0" group="General" />
                            </TunableTuple>
                        </TunableList>
                        <Tunable type="float" name="remove_value" class="Tunable" default="-100" display="Remove Value" muid="C832B35F3FF06E29" description="Track score value for the bit to be removed." tuning_state="NeedsTuning" Deprecated="False" filter="0" group="General" />
                    </TunableTuple>
                </TunableList>
                <Tunable type="statistic" name="y_axis_track" class="TunableReference" display="Y Axis Track" muid="CF132942A2762343" description="The bit track to key the Y axis off of." allow_none="False" Deprecated="False" filter="0" group="General" restrict="RelationshipTrack" />
            </TunableTuple>
            <TunableTuple name="bit_intervals" class="TunableTunableRelationshipIntervalBitData" display="Bit Intervals" muid="F0FC98AD68AFB3FF" description="Tuning to represent a track's bit tuning." Deprecated="False" filter="0" group="General">
                <TunableList name="entries" class="TunableList" display="Entries" muid="CA2220E7C1589327" description="The collection of bit intervals to use for this track." Deprecated="False" filter="0" group="General">
                    <TunableTuple class="TunableTunableRelationshipTrackBitInterval" muid="71D515FDC3C6A17C" description="Tuning to represent an interval of a track's score where the given bit is active." Deprecated="False" filter="0" group="General">
                        <Tunable type="relbit" name="bit" class="TunableReference" display="Bit" muid="E91AAC0F3C20351A" description="The bit that we want to use in this interval." allow_none="False" Deprecated="False" filter="0" group="General" />
                        <TunableTuple name="interval" class="TunableInterval" display="Interval" muid="1344918D98587FD0" description="The interval that this bit should be active for." Deprecated="False" filter="0" group="General">
                            <Tunable type="float" name="lower_bound" class="TunableRange" default="-100" display="Lower Bound" muid="4B94B32FE02A09FE" min="None" max="None" description="The lower bound of the interval." tuning_state="NeedsTuning" Deprecated="False" filter="0" group="General" />
                            <Tunable type="float" name="upper_bound" class="TunableRange" default="100" display="Upper Bound" muid="DDDA004D7D9EA872" min="None" max="None" description="The upper bound of the interval." tuning_state="NeedsTuning" Deprecated="False" filter="0" group="General" />
                        </TunableTuple>
                        <Tunable type="float" name="stickiness" class="Tunable" default="0" display="Stickiness" muid="8C52D3A3FD7B6886" description="How far past the interval boundaries should we continue&#xA;using this bit before switching to the next bit?&#xA;&#xA;When transitioning between intervals, it can be useful&#xA;to have the previous value 'stick' beyond the boundaries&#xA;of the interval. This can help prevent the bits from&#xA;switching too frequently if the track's value fluctuates&#xA;around an interval boundary.&#xA;&#xA;For example,&#xA;if we're on bit A, which is tuned for interval -10 to 10,&#xA;with a stickiness of 5, we won't switch away from A until&#xA;we're either below -15 or above 15.&#xA;Once we've left A, we won't re-enter A until we go above&#xA;-10 or below 10, assuming no other bits have stickiness." tuning_state="NeedsTuning" Deprecated="False" filter="0" group="General" />
                    </TunableTuple>
                </TunableList>
            </TunableTuple>
            <TunableTuple name="bit_set" class="TunableRelationshipBitData" display="Bit Set" muid="C51AAF537A2005C6" Deprecated="False" filter="0" group="General">
                <TunableList name="bit_data" class="TunableRelationshipBitSet" display="Bit Data" muid="62F4FC4A08AC23DA" Deprecated="False" filter="0" group="General">
                    <TunableTuple class="TunableTuple" muid="32F2E4B372058AC7" description="Data for this bit in the track" Deprecated="False" filter="0" group="General">
                        <Tunable type="float" name="add_value" class="Tunable" default="100" display="Add Value" muid="A1E2A3FD6AC1DA2B" description="Track score value for the bit to be added.&#xA;Since by default all relationships will converge at 0 we &#xA;must tune depending on the side of the zero we are in.&#xA;For values greater than 0, this must be greater than remove &#xA;value.&#xA;For values less than 0 this must be less than remove value. &#xA;&#xA;Example: For the friendship track:&#xA;GOOD_FRIENDS (value&gt;0) has an add value of 60&#xA;As soon as the track value goes &gt;= 60 the bit good friends&#xA;will be added and the previous active track bit will be removed.&#xA;&#xA;DISLIKED (value&lt;0) has an add_value of -20&#xA;As soon as the track value goes &lt;= -20 the bit disliked will &#xA;be added and the previous active track bid will be removed.&#xA;&#xA;TUNING MIDDLE VALUES (Ranges approach 0)&#xA;When tuning a value that goes past 0 (a bit from 10 to -10) it&#xA;is recommended we tune a positive Bit (10 to 0) and a negative &#xA;bit (-10 to 0).  This way, we can guarantee the rules will &#xA;consider correct positive and negative directions." tuning_state="NeedsTuning" Deprecated="False" filter="0" group="General" />
                        <Tunable type="relbit" name="bit" class="TunableReference" display="Bit" muid="F5985A5537A4CEE6" description="Reference to bit in set" allow_none="False" Deprecated="False" filter="0" group="General" />
                        <Tunable type="float" name="remove_value" class="Tunable" default="-100" display="Remove Value" muid="8F9D913FCD447775" description="Track score value for the bit to be removed.&#xA;Since by default all relationships will converge at 0 we &#xA;must tune depending on the side of the zero we are in.&#xA;For values greater than 0, this must be less than add value.&#xA;For values less than 0, this must be greater than add value. &#xA;&#xA;For example, on the friendship track:&#xA;GOOD_FRIENDS (value&gt;0) has a remove value of 55.&#xA;As soon as the track value goes below 55 the bit good friends&#xA;will be removed, and the next lowest bit will be added.&#xA;&#xA;DISLIKED (value&lt;0) has a remove_value of -15.&#xA;As soon as the track value goes over -15 the bit disliked will &#xA;be removed and the next highest bit will be added.&#xA;&#xA;TUNING MIDDLE VALUES (Ranges approach 0)&#xA;When tuning a value that goes past 0 (a bit from 10 to -10) it&#xA;is recommended we tune a positive Bit (10 to 0) and a negative &#xA;bit (-10 to 0).  This way, we can guarantee the rules will &#xA;consider correct positive and negative directions." tuning_state="NeedsTuning" Deprecated="False" filter="0" group="General" />
                    </TunableTuple>
                </TunableList>
            </TunableTuple>
            <Tunable name="none" class="TunableExistance" display="None" muid="F0D822B7756D5ACA" />
        </TunableVariant>

